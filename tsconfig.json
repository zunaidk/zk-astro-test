{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@layouts/*": [
        "./layouts/*"
      ],
      "@lib/*": [
        "./lib/*"
      ]
    }
  }
}